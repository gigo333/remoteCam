         <font>
            <Font size="24.0" />
         </font></Label>
      <Pane prefHeight="87.0" prefWidth="302.0" />
      <Button fx:id="primaryButton" alignment="CENTER" onAction="#switchToSecondary" prefHeight="37.0" prefWidth="142.0" text="Start Server" />
